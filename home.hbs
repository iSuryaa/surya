<h3>Student Managemet System</h3><br><br>
<form class="search">
  <input type="text" placeholder="Search.." id="search" name="search" ><button type="submit" ><i class="fa fa-search"></i></button>
</form>

<p><a href="/adduser" class="addbtn"> ADD</a></p><br><br>

<div class="table-items">
<table>
    <thead>
        <tr>
            <td>ID</td>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Email</td>
            <td>DOB</td>
            <td>Education</td>
            <td>About</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
{{#each rows}}
<tr>
    <td>{{this.ID}}</td>
    <td>{{this.FirstName}}</td>
    <td>{{this.LastName}}</td>
    <td>{{this.Email}}</td>
    <td>{{this.DOB}}</td>
    <td>{{this.Education}}</td>
    <td>{{this.About}}</td>
    
    <td><i style='font-size:18px' class='far'>&#xf044;</i><a class="edit" href="/edituser/{{this.ID}}">Edit </a></td>
    <td><i style='font-size:18px' class='fas'>&#xf2ed;</i><a onclick="return confirm('Are you sure?')" class="delete" href="/deleteuser/{{this.ID}}">Delete</a></td>
</tr>
{{/each}}
    </tbody>
    </table>

    </div>